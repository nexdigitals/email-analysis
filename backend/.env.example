# Copy to `.env` and fill in your own values (keep secrets out of version control).

# OpenAI (optional, for AI-assisted analysis)
OPENAI_API_KEY=your-openai-key
OPENAI_MODEL=gpt-4o-mini

# API auth (optional)
API_TOKEN=

# Analyzer tuning (optional)
ANALYZER_TIMEOUT_SEC=12
ANALYZER_PLAYWRIGHT_TIMEOUT_SEC=20
ANALYZER_MAX_WORKERS=8
MAX_UPLOAD_BYTES=5000000
ANALYZER_MAX_CONTENT_BYTES=2000000
ANALYZER_ALLOW_PRIVATE_IP=0

# MongoDB (optional)
MONGODB_URI=
MONGODB_DB=analyzer
MONGODB_COLLECTION=results
